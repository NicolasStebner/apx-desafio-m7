{
  "name": "capitulo-4-orm-sequelize",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:fe": "parcel serve ./fe-src/index.html",
    "dev:be": "nodemon --watch ./be-src/index.ts --exec ts-node ./be-src/index.ts",
    "build:be": "tsc --skipLibCheck ./be-src/index.ts",
    "build:fe": "parcel build ./fe-src/index.html",
    "build": "npm run build:fe && npm run build:be",
    "dev": "npm run dev:be",
    "start": "node ./be-src/index.js"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "algoliasearch": "^4.20.0",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "nodemon": "^3.0.1",
    "parcel": "^2.9.3",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "resend": "^2.0.0",
    "sequelize": "^6.32.1",
    "typescript": "^5.1.6"
  }
}
